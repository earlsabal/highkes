<!DOCTYPE html>
<html>
<head>
    <%= render "layouts/meta_tags" %>
    <%= render "layouts/flash_msg" %>
    <%= render "layouts/template" %>
</head>
<body>
    <%= render "layouts/nav_bar" %>




    <div class="container">

      <div class="starter-template">
            <% if notice %>
              <p class="alert alert-success"><%= notice %></p>
            <% end %>
            <% if alert %>
              <p class="alert alert-danger"><%= alert %></p>
            <% end %>
            <%= yield %>
      </div>
    </div>


         


  <%= render "layouts/javascript" %>
</body>
</html>
